{
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "X-Accel-Expires",
          "value": "0"
        }
      ],
      "source": "/api/(.*)"
    }
  ],
  "regions": ["fra1"]
}
